{
  "expo": {
    "name": "PokemonInfoApp",
    "slug": "PokemonInfoApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pokeball-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/pokeball-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF3334"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/pokeball-icon.png",
        "backgroundColor": "#FF3334"
      }
    },
    "web": {
      "favicon": "./assets/images/pokeball-icon.png"
    }
  }
}
